{
  "name": "chord-rpc",
  "version": "0.1.8",
  "author": "Din Dmitriy @dmdin",
  "description": "ðŸ’Ž Cutting edge transport framework vanishing borders between frontend and backend",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dmdin/chord.git"
  },
  "keywords": [
    "JSON-RPC",
    "Client Server",
    "Transport"
  ],
  "license": "Apache License 2.0",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./middlewares/*": "./dist/middlewares/*.js",
    "./specs/": "./dist/specs/index.js"
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "middlewares/*": [
        "./dist/middlewares/*.d.ts"
      ],
      "specs": [
        "./dist/specs/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write ./src/**/*.{js,json,html,ts}",
    "lint": "eslint --fix .",
    "dev": "nodemon --exec npx ts-node src/tests.ts",
    "patch": "npm version patch --no-git-tag-version",
    "publish": "npm publish --no-git-checks"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  }
}
